@using MVCOnlineTicariOtomasyon.Models.SINIFLAR
@model List<FaturaKalem>
@{
    ViewBag.Title = "FaturaDetay";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<br />
<h2>Fatua Detay Sayfası</h2>
<br />
<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active"></li>
</ol>

<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>AÇIKLAMA</th>
        <th>MİKTAR</th>
        <th>BİRİM FİYAT</th>
        <th>TUTAR</th>
    </tr>
    @foreach (var x in Model)
    {


        <tr>
            <td>@x.FaturaaID</td>
            <td>@x.Açıklama</td>
            <td>@x.Miktar</td>
            <td>@x.BirimFiyat</td>
            <td>@x.Tutar</td>
            
        </tr>
    }
</table>
<a href="/Fatura/YeniKalem/" class="btn btn-default"style="background-color:darksalmon">YENİ KALEM GİRİŞİ</a>

