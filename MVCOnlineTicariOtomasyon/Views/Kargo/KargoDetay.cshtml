@using MVCOnlineTicariOtomasyon.Models.SINIFLAR
@model List<KargoDetay>
@{
    ViewBag.Title = "KargoDetay";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}




<br />
<h2>Kargo Detay </h2>
<br />
<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active"></li>
</ol>

<table class="table table-hover">
    <tr>
        <th>TAKİP KODU</th>
        <th>PERSONEL</th>
        <th>ALICI</th>
        <th>TARİH</th>
        
    </tr>
    @foreach (var x in Model)
    {


        <tr>
            <td>@x.TakipKodu</td>
            <td>@x.Personel</td>
            <td>@x.Alıcı</td>
            <td>@x.Tarih</td>
            

        </tr>
    }
</table>
<a href="/Kargo/YeniKargo/" class="btn btn-default" style="background-color:darksalmon">YENİ KARGO GİRİŞİ</a>



